<div id="sponsored_list">
<table>
<tr><th>Bill</th><th>Status</th><th>Last Action</th></tr>
<% sponsored_list.each do |bill| -%>
<tr><td><%= link_to(bill.typenumber, bill_path(bill)) %> <%= bill.title_common %></td><td><%=bill.status %></td><td><%= bill.last_action.formatted_date %></td></tr>
<% end -%>
</table>

</div>

